    "@types/node": "^22.7.6",
    "csv-parser": "^3.0.0",
    "jest": "^29.7.0",
    "prisma": "^5.21.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@types/aws-lambda": "^8.10.145",
    "aws-sdk-client-mock": "^4.1.0",
    "dotenv-cli": "^7.4.2",
    "prismock": "^1.33.3",
    "typescript": "^5.5.3"
  },
